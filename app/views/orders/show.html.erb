<h1>Order#show</h1>
<p>Find me in app/views/orders/show.html.erb</p>

<table >
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Price</th>
      <th>Image url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
   <%= @item_orders.each do |item_order | %>
    <tr>
        <td><%= item_order.item.title %></td>
        <td><%= item_order.item.price %></td>
    </tr>

  <% end %>
  </tbody>
</table>
